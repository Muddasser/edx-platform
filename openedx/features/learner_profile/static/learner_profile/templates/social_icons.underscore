<div class="social-links">
    <% for (var platform in socialLinks) { %>
        <% if (ownProfile || socialLinks[platform]) { %>
            <% if (socialLinks[platform]) { %> <a target="_blank" href= <%-socialLinks[platform]%>><% } %>
                <span class="icon fa fa-<%-platform%>-square" data-platform=<%-platform%> aria-hidden="true"></span>
            <% if (socialLinks[platform]) { %> </a> <% } %>
        <% } %>
    <% } %>
</div>
